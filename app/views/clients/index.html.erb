<div class="col-md-12 text-center marginBottom50">
	<%= link_to "< ", admins_path, class: 'btn btn-default' %>
	<%= link_to 'ADD A NEW CLIENTS', new_client_path, class: 'btn btn-success' %>
</div>
<div id="admin-clients-index">
	<div class="container">
		<div class="row">
			<% @clients.each do |client| %>
				<div class="col-md-3">
					<%= image_tag client.image.url(:square) %>
					<%= link_to 'Delete', client, data: { confirm: 'Are you sure?' }, method: :delete, remote: true %>
				</div>
			<% end %>
		</div>
	</div>
</div>
	